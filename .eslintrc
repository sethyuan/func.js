{
  "parser": "babel-eslint",
  "plugins": [
    "babel"
  ],
  "env": {
    "es6": true
  },
  "rules": {
    "object-shorthand": 0,
    "generator-star-spacing": 0,
    "babel/object-shorthand": 1,
    "babel/generator-star-spacing": [1, "after"],
    "no-var": 2,
    "strict": 0,
    "indent": [2, 2],
    "quotes": [2, "double"],
    "linebreak-style": [2, "unix"],
    "semi": [2, "always"],
    "curly": [2, "multi-line"],
    "no-shadow": 1,
    "no-constant-condition": 0,
  }
}
